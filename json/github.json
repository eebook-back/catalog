{
    "name": "github",
    "author": "knarfeh",
    "author_link": "https://github.com/knarfeh",
    "repo": "https://github.com/eebook/githubeebook",
    "repo_type": "github",
    "info": "Convert github issue to ebook",
    "regex": "(?<=github\\.com/)",
    "image": "knarfeh/githubissueseebook",
    "image_version": "latest",
    "schema": {
        "properties": {
            "GITHUB_TOKEN": {
                "type": "string",
                "title": "Github token",
                "description": "github token"
            },
            "QUERY_STRING": {
                "type": "string",
                "title": "query string",
                "description": "query string, like state=open"
            }
        },
        "required": ["GITHUB_TOKEN", "QUERY_STRING" ]
    }
}
