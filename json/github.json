{
    "name": "github",
    "author": "knarfeh",
    "author_link": "https://github.com/knarfeh",
    "repo": "https://github.com/knarfeh/githubissueseebook",
    "repo_type": "github",
    "info": "Convert github issue to ebook",
    "regex": "(?<=github\\.com/)",
    "image": "knarfeh/githubissueseebook",
    "image_version": "latest",
    "schema": {
        "properties": {
            "GITHUB_TOKEN": {
                "type": "string",
                "title": "Github token",
                "description": "github token"
            },
            "QUERY_STRING": {
                "type": "string",
                "title": "query string",
                "default": "state=all",
                "description": "query string, like state=open"
            },
            "CONTENT_IS_MARKDOWN": {
                "description": "content is markdown",
                "title": "Content is Markdown",
                "type": "boolean",
                "default": true,
                "readOnly": true
            }
        },
        "required": ["GITHUB_TOKEN"]
    }
}
