{
    "title": "catalog",
    "description": "Metadata schema for eebook catalog",
    "type": "object",
    "$schema": "http://json-schema.org/draft-04/schema",
    "required": [
        "name",
        "repo",
        "info",
        "regex",
        "image",
        "image_version"
    ],
    "properties": {
        "name": {
            "type": "string"
        },
        "repo": {
            "type": "string"
        },
        "info": {
            "type": "string"
        },
        "regex": {
            "type": "string"
        },
        "image": {
            "type": "string"
        },
        "image_version": {
            "type": "string"
        },
        "schema": {
            "type": "object"
        }
    }
}
